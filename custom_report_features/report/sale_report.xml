<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_report_totalval" model="ir.actions.report">
        <field name="name">Quotation with Total</field>
        <field name="model">sale.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">custom_report_features.report_saleorder_total</field>
        <field name="report_file">custom_report_features.report_saleorder_total</field>
        <field name="print_report_name">'Quotation SOL - %s' % (object.name)</field>
        <field name="binding_model_id" ref="model_sale_order" />
        <field name="binding_type">report</field>
    </record>

